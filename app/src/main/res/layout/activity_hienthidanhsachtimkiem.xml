<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".hienthidanhsachtimkiem"
    android:orientation="vertical">
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.SearchView
            android:layout_width="350dp"
            app:searchIcon="@null"
            android:background="@drawable/menu_seach"
            android:id="@+id/svdstk"
            android:layout_height="wrap_content"></androidx.appcompat.widget.SearchView>

    </androidx.appcompat.widget.Toolbar>
    <Spinner
        android:layout_marginTop="10dp"
        android:id="@+id/spdstk"


        android:layout_width="200dp"
        android:layout_height="wrap_content"></Spinner>
    <ListView
        android:layout_marginTop="10dp"
        android:id="@+id/lvdstk"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></ListView>

</LinearLayout>